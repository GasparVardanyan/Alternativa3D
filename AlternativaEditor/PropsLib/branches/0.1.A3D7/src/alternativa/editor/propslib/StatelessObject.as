package alternativa.editor.propslib {
	
	public class StatelessObject extends ObjectContainer {
		
		/**
		 * Список LOD'ов объекта.
		 */		
		public var lods:Array;
		
		public function StatelessObject(object:PropObject, lods:Array) {
			super(object);
			this.lods = lods;
		}
		
		public function getLodByDistance(distance:Number):PropLOD {
			return null;
		}
		
		public function toString():String {
			return "[StatelessObject object=" + object + ", lods=" + lods + "]";
		}

	}
}